{
  "compilerOptions": {
    "module": "amd",
    "noImplicitAny": true,
    "sourceMap": false,
    "jsx": "react",
    "jsxFactory": "tsx",
    "target": "es5",
    "outDir": "./dist/",
    "experimentalDecorators": true,
    "preserveConstEnums": true,
    "suppressImplicitAnyIndexErrors": true
  },
  "include": ["./src/**/*"],
  "exclude": ["node_modules"]
}
